	keywords = {Animals, Brain, Cell Nucleus, Gene Expression Profiling, Gene Expression Regulation, Developmental, HEK293 Cells, Humans, Mice, Neurons, NIH 3T3 Cells, Sequence Analysis, RNA, Single-Cell Analysis, Spinal Cord, Transcriptome},
	pages = {176--182},
	file = {PubMed Central Full Text PDF:/Users/amirebrahimi/Zotero/storage/HX3Y5XT3/Rosenberg et al. - 2018 - Single-cell profiling of the developing mouse brain and spinal cord with split-pool barcoding.pdf:application/pdf},
