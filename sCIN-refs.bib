	file = {PubMed Central Full Text PDF:/Users/amirebrahimi/Zotero/storage/T9TXHWR4/Chen et al. - 2019 - High-throughput sequencing of the transcriptome and chromatin accessibility in the same cell.pdf:application/pdf},
}

